      "description": "Access tokens allow a user to authenticate against the Pulumi Service",
      "properties": {
        "tokenId": {
          "description": "The token identifier.",
          "type": "string"
        },
        "description": {
          "description": "Description of the access token.",
          "type": "string"
        },
        "value": {
          "description": "The token's value.",
          "type": "string",
          "secret": true
        }
      },
      "inputProperties": {
        "description": {
          "description": "Description of the access token.",
          "type": "string"
        }
      },
      "requiredInputs": ["description"]
    },
    "pulumiservice:index:Team": {
      "description": "The Pulumi Service offers role-based access control (RBAC) using teams. Teams allow organization admins to assign a set of stack permissions to a group of users.",
      "properties": {
        "teamType": {
          "description": "The type of team. Must be either `pulumi` or `github`.",
          "type": "string"
        },
        "name": {
          "description": "The team name.",
          "type": "string"
        },
        "displayName": {
          "description": "Optional. Team display name.",
          "type": "string"
        },
        "description": {
          "description": "Optional. Team description.",
          "type": "string"
        },
        "members": {
          "description": "List of team members.",
          "type": "array",
