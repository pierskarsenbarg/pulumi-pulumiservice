    "pulumiservice:index:Webhook": {
      "description": "Pulumi Webhooks allow you to notify external services of events happening within your Pulumi organization or stack. For example, you can trigger a notification whenever a stack is updated. Whenever an event occurs, Pulumi will send an HTTP POST request to all registered webhooks. The webhook can then be used to emit some notification, start running integration tests, or even update additional stacks.",
      "properties": {
        "name": {
          "description": "Webhook identifier generated by pulumi service.",
          "type": "string"
        },
        "active": {
          "description": "Indicates whether this webhook is enabled or not.",
          "type": "boolean"
        },
        "displayName": {
          "description": "The friendly name displayed in the Pulumi Service.",
          "type": "string"
        },
        "payloadUrl": {
          "description": "URL to send request to.",
          "type": "string"
        },
        "secret": {
          "description": "Optional. secret used as the HMAC key. See [webhook docs](https://www.pulumi.com/docs/intro/pulumi-service/webhooks/#headers) for more information.",
          "type": "string",
          "secret": true
        },
        "organizationName": {
          "description": "Name of the organization.",
          "type": "string"
        }
      },
